{
    "require": {
        "Yubico/php-yubico": "dev-master",
        "xmlseclibs": "dev-master",
        "yubico/u2flib-server": "dev-master"
    },
    "repositories": [
        {
            "type": "pear",
            "url": "http://pear2.php.net"
        },
        {
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"Yubico/php-yubico",
                "source": {
                    "url":"https://github.com/Yubico/php-yubico.git",
                    "type":"git",
                    "reference":"master"
                },
                "dist": {
                    "url":"https://github.com/Yubico/php-yubico/zipball/master",
                    "type":"zip"
                }
            }
        },
	{
            "type":"package",
            "package": {
                "version":"dev-master",
                "name":"xmlseclibs",
                "dist": {
                    "url":"https://xmlseclibs.googlecode.com/files/xmlseclibs-1.3.1.tar.gz",
                    "type":"tar"
                }
            }
	}
    ]
}
